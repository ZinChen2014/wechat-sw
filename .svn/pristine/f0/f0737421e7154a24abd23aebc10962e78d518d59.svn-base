<#include "/pages/common/_layoutForMsg.html"/>
<@layout>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<script src="/../../js/weixin/unbindUser.js"></script>
<title>取消绑定</title>
</head>
<body>
<div style="margin:5px;">
	<div class="panel panel-info">
		<div class="panel-heading">
			<ul class="nav nav-pills nav-justified">
				<li>取消绑定</li>
			</ul>
		</div>
		<div class="panel-body" style="padding:8px">
			<table class="table-bordered table-condensed" width="100%">
				<tr>
					<td width="30%" align="center">绑定日期</td>
					<td width="20%" align="center">客户号</td>
					<td width="23%" align="center">绑定用户</td>
					<td width="27%" align="center"></td>
				</tr>
				  <#list objList as obj>	
				<tr>
					<td>${obj.createTime?string('yyyy-MM-dd')}</td>
					<td>${obj.userNo}</td>
					<td>${obj.userName}</td>
					<td id="" colspan="2" align="center"><a href="javascript:void(0)" onclick="unbind('${obj.openID}','${obj.userNo}')">解除绑定</a></td>
				</tr>
				 </#list>
			</table>
		</div>
	</div>
</div>
</@layout>