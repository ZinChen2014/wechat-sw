<#include "/pages/common/_layoutForMsg.html"/>
<@layout>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<script src="/../../js/weixin/myUsedWater.js"></script>
<title>我的用水</title>
</head>

<body>
<div class="main">
 <#list result as myWaterInfo>
 	<#if myWaterInfo_index<1>
	<table border="1" class="list${myWaterInfo_index}" indexlist="${myWaterInfo_index}" indexall="${result?size}">
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">户号</td>
			<td colspan="2">${myWaterInfo.userNo?default("--")}</td>
			<td bgcolor="#D9D9D9">水表卡号</td>
			<td>${myWaterInfo.cardNo?default("--")}</td>
			<td colspan="2"><a  class="next_shuibiao">下一个水表</a></td>
		</tr>
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">户名</td>
			<td colspan="4">${myWaterInfo.userName?default("--")}</td>
			<td bgcolor="#D9D9D9">预存款</td>
			<td>${myWaterInfo.rePay?default("0.00")}</td>
		</tr>
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">地址</td>
			<td colspan="6" >${myWaterInfo.address?default("--")}</td>
		</tr>		
		<#list myWaterInfo.waterList as monthWaterInfo>
		<tr class="trh trbg">
			<td >月份</td>
			<td >上期数</td>
			<td >本期数</td>
			<td >实抄数</td>
			<td >应收数</td>
			<td >金额</td>
			<td >违约金</td>
			<td >缴费否</td>
		</tr>
		<#if monthWaterInfo.flag=="1" >
		<tr class="trh">
			<td >${monthWaterInfo.yearMonth?default("--")}</td>
			<td >${monthWaterInfo.agoAmount?default("0")}</td>
			<td >${monthWaterInfo.currentAmount?default("0")}</td>
			<td >${monthWaterInfo.amount?default("0")}</td>
			<td >${monthWaterInfo.mustAmount?default("0")}</td>
			<td >${monthWaterInfo.fee?default("0.00")}</td>
			<td >${monthWaterInfo.wxFee?default("0.00")}</td>
			<td >已缴</td>
		</tr>
		<#else>
		<tr class="trh" style="color:red">
			<td >${monthWaterInfo.yearMonth?default("--")}</td>
			<td >${monthWaterInfo.agoAmount?default("0")}</td>
			<td >${monthWaterInfo.currentAmount?default("0")}</td>
			<td >${monthWaterInfo.amount?default("0")}</td>
			<td >${monthWaterInfo.mustAmount?default("0")}</td>
			<td >${monthWaterInfo.fee?default("0.00")}</td>
			<td >${monthWaterInfo.wxFee?default("0.00")}</td>
			<td ><a href="javascript:void(0)" onclick="goForUrl()">未缴</a></td>
		</tr>
		</#if>
		</#list>
		
	</table>
	<#else>
	<table border="1" class="list${myWaterInfo_index}" indexlist="${myWaterInfo_index}" indexall="${result?size}" style="display:none;">
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">户号</td>
			<td colspan="2">${myWaterInfo.userNo?default("--")}</td>
			<td bgcolor="#D9D9D9">水表卡号</td>
			<td>${myWaterInfo.cardNo?default("--")}</td>
			<td colspan="2"><a  class="next_shuibiao">下一个水表</a></td>
		</tr>
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">户名</td>
			<td colspan="4">${myWaterInfo.userName?default("--")}</td>
			<td bgcolor="#D9D9D9">预存款</td>
			<td>${myWaterInfo.rePay?default("0.00")}</td>
		</tr>
		<tr>
			<td colspan="2" bgcolor="#D9D9D9">地址</td>
			<td colspan="6" >${myWaterInfo.address?default("--")}</td>
		</tr>			
		<#list myWaterInfo.waterList as monthWaterInfo>
		
		<tr class="trh trbg">
			<td >月份</td>
			<td >上期数</td>
			<td >本期数</td>
			<td >实抄数</td>
			<td >应收数</td>
			<td >金额</td>
			<td >违约金</td>
			<td >缴费否</td>
		</tr>
			<#if monthWaterInfo.flag=="1" >
		<tr class="trh">
			<td >${monthWaterInfo.yearMonth?default("--")}</td>
			<td >${monthWaterInfo.agoAmount?default("0")}</td>
			<td >${monthWaterInfo.currentAmount?default("0")}</td>
			<td >${monthWaterInfo.amount?default("0")}</td>
			<td >${monthWaterInfo.mustAmount?default("0")}</td>
			<td >${monthWaterInfo.fee?default("0.00")}</td>
			<td >${monthWaterInfo.wxFee?default("0.00")}</td>
				<td >已缴</td>
		</tr>
		<#else>
		<tr class="trh" style="color:red">
			<td >${monthWaterInfo.yearMonth?default("--")}</td>
			<td >${monthWaterInfo.agoAmount?default("0")}</td>
			<td >${monthWaterInfo.currentAmount?default("0")}</td>
			<td >${monthWaterInfo.amount?default("0")}</td>
			<td >${monthWaterInfo.mustAmount?default("0")}</td>
			<td >${monthWaterInfo.fee?default("0.00")}</td>
			<td >${monthWaterInfo.wxFee?default("0.00")}</td>
				<td ><a href="javascript:void(0)" onclick="goForUrl()">未缴</a></td>
		</tr>
		</#if>
		</#list>
		
	</table>
	</#if>
	 </#list>
	 
</div>
</@layout>