<#include "/pages/common/_layout.html"/>
<@layout>
<title>停水消息编辑</title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
 <script type="text/javascript">
</script>
<script src="${CONTEXT_PATH}/js/regularExpressionCommon.js"></script>
   <script src="${CONTEXT_PATH}/js/user/editShutOffWaterInfo.js"></script>

  <!-- content start -->
    <div class="am-cf am-padding">
      <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">停水通知</strong> / <small>编辑</small></div>
    </div>

    <hr/>

    <div class="am-g">

      <div class="am-u-sm-12 am-u-md-4 am-u-md-push-8">
     
      </div>

      <div class="am-u-sm-12 am-u-md-8 am-u-md-pull-4">
        <form class="am-form am-form-horizontal">
          <div class="am-form-group">
            <label for="user-name" class="am-u-sm-3 am-form-label">停水类型 / keyword1</label>
            <div class="am-u-sm-9">
              <input type="text" readonly id="keyword1" value="" placeholder="停水类型 / keyword1">
             <!--  <small>输入你的名字，让我们记住你。</small> -->
            </div>
          </div>

          <div class="am-form-group">
            <label for="user-password" class="am-u-sm-3 am-form-label">停水时间 / keyword2</label>
            <div class="am-u-sm-9">
              <input type="text"  id="keyword2" placeholder="输入停水时间范围 / keyword2">
              <!-- <small></small> -->
            </div>
          </div>

           <div class="am-form-group">
            <label for="user-password" class="am-u-sm-3 am-form-label">停水区域 / keyword3</label>
            <div class="am-u-sm-9">
              <input type="text" id="keyword3" placeholder="输入停水区域 / keyword3">
              <!-- <small>新的登录密码</small> -->
            </div>
          </div>

         <div class="am-form-group">
            <label for="user-intro" class="am-u-sm-3 am-form-label">附加信息 / remark</label>
            <div class="am-u-sm-9">
              <textarea class="" rows="5" id="remark" placeholder="输入你需要补充说明的信息 / remark"></textarea>
              <!-- <small>250字以内写出你的一生...</small> -->
            </div>
          </div>

          <div class="am-form-group">
            <div class="am-u-sm-9 am-u-sm-push-3">
              <button type="button" class="am-btn am-btn-primary" id="submit">保存修改</button>
            </div>
          </div>
        </form>
      </div>
    </div>

</@layout>
