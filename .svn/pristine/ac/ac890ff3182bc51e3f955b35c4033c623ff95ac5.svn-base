$(function(){
	$(".next_shuibiao").click(function(){
		var now = $(this).parents("table").attr("indexlist");
		if($(this).parents("table").attr("indexall")-1==now){
			$(".list0").show().siblings().hide();			
		}else{
			$(this).parents("table").next("table").show().siblings().hide();
		}
	});
});