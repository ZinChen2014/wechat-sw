<#include "/pages/common/_layout.html"/>
<@layout>
<title>停水记录</title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
 <script type="text/javascript">
	var HREFURL = "/admin/getShutOffWaterInfoList/";
</script>
  <script src="${CONTEXT_PATH}/js/user/shutOffWaterInfo.js"></script>
    <div class="am-cf am-padding">
      <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">停水消息</strong> / <small>列表管理</small></div>
   </div>
	
		<div class="am-g">
		<div class="am-u-sm-6 am-u-md-6">
		  <div class="am-btn-toolbar">
	          <div class="am-btn-group am-btn-group-xs">
	            <button type="button" class="am-btn am-btn-default" onclick="sendShutOffWaterInfoFunc()"><span class="am-icon-archive"></span>发送停水信息 </button>
	          </div>
	        </div> 
		</div>
		</div>
		<br>

<hr>
    <div class="am-g" >
      <#if !prjPage?has_content>
          		 查询没有数据
            <#else>
      <div class="am-u-sm-12">
       
        <form class="am-form">
          <table class="am-table am-table-striped am-table-hover table-main">
            <thead>
              <tr>
                <#-- <th class="table-check"><input type="checkbox" /></th> -->
                <th class="table-id">ID</th>
                <th class="table-openID">停水类型</th>
                <th class="table-userNo">停水区域</th>
                 <th class="table-userName">停水时间</th>
                <th class="table-phoneNum am-hide-sm-only">模板ID</th>
                 <th class="table-address am-hide-sm-only">备注</th>
                <th class="table-createTime am-hide-sm-only">绑定日期</th>
               <#--  <th class="table-set">操作</th> -->
              </tr>
          </thead>
          <tbody>
        
          <#list prjPage.getList() as x>
            <tr>
             <#--  <td><input type="checkbox" /></td>-->
              <td>${x.id}</td>
              <td><a href="#">${x.keyword1?default('')}</a></td>
              <td>${x.keyword3}</td>
              <td><a href="#">${x.keyword2?default('')}</a></td>
              <td class="am-hide-sm-only">${x.mouldID?default('')}</td>
               <td class="am-hide-sm-only"><a href="#">${x.remark?default('')}</a></td>
              <td class="am-hide-sm-only">${x.createTime?default('')}</td>
                 <#--   
              <td>
                <div class="am-btn-toolbar">
                  <div class="am-btn-group am-btn-group-xs">
                    <button class="am-btn am-btn-default am-btn-xs am-text-secondary"><span class="am-icon-pencil-square-o"></span> 编辑</button>
                   <button class="am-btn am-btn-default am-btn-xs am-text-danger am-hide-sm-only"> <a href="/user/delBindingUser/${x.id}"><span class="am-icon-trash-o"></span> 删除</a></button>
                  </div>
                </div>
              </td>-->
            </tr>
           </#list>
      
          </tbody>
        </table>
       
        <#include "/pages/common/_paginate.html" />
	<@paginate currentPage=prjPage.pageNumber totalPage=prjPage.totalPage
	 actionUrl="/admin/getShutOffWaterInfoList/" />
          <hr />
        </form>
         
      </div>
    	  
</#if>
    </div>
  <#-- content end -->
</@layout>
