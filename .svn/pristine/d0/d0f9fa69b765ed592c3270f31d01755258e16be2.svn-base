<#include "/pages/common/_layout.html"/>
<@layout>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
 <script type="text/javascript">
	var HREFURL = "/user/getBindingUserList/";
</script>
  <script src="${CONTEXT_PATH}/js/user/userbind.js"></script>
    <div class="am-cf am-padding">
      <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">绑定用户</strong> / <small>列表管理</small></div>
   </div>
	
	<div class="am-g">
		<div class="am-u-sm-12 am-u-md-6">
			<!--  <div class="am-btn-toolbar">
	          <div class="am-btn-group am-btn-group-xs">
	            <button type="button" class="am-btn am-btn-default"><span class="am-icon-plus"></span> 新增</button>
	            <button type="button" class="am-btn am-btn-default"><span class="am-icon-save"></span> 保存</button>
	            <button type="button" class="am-btn am-btn-default"><span class="am-icon-archive"></span> 审核</button>
	            <button type="button" class="am-btn am-btn-default"><span class="am-icon-trash-o"></span> 删除</button>
	          </div>
	        </div> -->
		</div>
		<div class="am-u-sm-12 am-u-md-6">
			<div class="am-alert am-alert-danger" id="my-alert"
				style="display: none">
				<p>开始日期应小于结束日期！</p>
			</div>
			<div class="am-g">
				<div class="am-u-sm-6">
					<input type="text" id="startDate" class="am-form-field"
						placeholder="开始日期" data-am-datepicker readonly required />
				</div>
				<div class="am-u-sm-6">
					<input type="text" id="endDate" class="am-form-field"
						placeholder="结束日期" data-am-datepicker readonly required />
				</div>
			</div>
	
		</div>
		<div class="am-u-sm-12 am-u-md-4">
			<span class="am-u-md-3"> 户号： </span>
			<div class="am-input-group am-input-group-sm ">
				<input type="text" id="userNo" class="am-form-field "> <span
					class="am-input-group-btn">
					<button class="am-btn am-btn-default" type="button" id="search">搜索</button>
				</span>
			</div>
		</div>
	</div>
<hr>
    <div class="am-g" >
      <#if !prjPage?has_content>
          		 查询没有数据
            <#else>
      <div class="am-u-sm-12">
       
        <form class="am-form">
          <table class="am-table am-table-striped am-table-hover table-main">
            <thead>
              <tr>
                <#-- <th class="table-check"><input type="checkbox" /></th> -->
                <th class="table-id">ID</th>
                <th class="table-openID">用户微信Id</th>
                <th class="table-userNo">户号</th>
                 <th class="table-userName">户主</th>
                <th class="table-phoneNum am-hide-sm-only">手机号码</th>
                 <th class="table-address am-hide-sm-only">地址</th>
                <th class="table-createTime am-hide-sm-only">绑定日期</th>
               <#--  <th class="table-set">操作</th> -->
              </tr>
          </thead>
          <tbody>
        
          <#list prjPage.getList() as x>
            <tr>
             <#--  <td><input type="checkbox" /></td>-->
              <td>${x.id}</td>
              <td><a href="#">${x.openID?default('')}</a></td>
              <td>${x.userNo}</td>
              <td><a href="#">${x.userName?default('')}</a></td>
              <td class="am-hide-sm-only">${x.phoneNum?default('')}</td>
               <td class="am-hide-sm-only"><a href="#">${x.address?default('')}</a></td>
              <td class="am-hide-sm-only">${x.createTime?default('')}</td>
                 <#--   
              <td>
                <div class="am-btn-toolbar">
                  <div class="am-btn-group am-btn-group-xs">
                    <button class="am-btn am-btn-default am-btn-xs am-text-secondary"><span class="am-icon-pencil-square-o"></span> 编辑</button>
                   <button class="am-btn am-btn-default am-btn-xs am-text-danger am-hide-sm-only"> <a href="/user/delBindingUser/${x.id}"><span class="am-icon-trash-o"></span> 删除</a></button>
                  </div>
                </div>
              </td>-->
            </tr>
           </#list>
      
          </tbody>
        </table>
       
        <#include "/pages/common/_paginate.html" />
	<@paginate currentPage=prjPage.pageNumber totalPage=prjPage.totalPage
	 actionUrl="/admin/getBindingUserList/" 
	urlParas="&startDate="+startDate+
	"&userNo="+userNo+
	"&endDate="+endDate />
          <hr />
        </form>
         
      </div>
    	  
</#if>
    </div>
  <#-- content end -->
</@layout>
