function chkInputMonery(money){
	if (money == null || $.trim(money) == "" || "0.00"==money || 0==money) {
		alert("输入金额");
		return false;
	}
	var re = /^([0-9]\d{0,5})(\.(\d)?(\d)?)?$/; 	
	if (!re.test(money)) {
		alert("请输入金额到分");		
		return false;
	}
}




